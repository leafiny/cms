{# Frontend_Block_Resources #}
<meta charset="{{ block.getCustom('charset') }}">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>{{ page.getCustom('meta_title')|translate }}</title>
<meta name="description" content="{{ page.getCustom('meta_description')|translate }}" />
{% for css in block.getStylesheets(page) %}
<link rel="stylesheet" href="{{ css }}" />
{% endfor %}
{% for js in block.getScripts(page) %}
<script type="text/javascript" src="{{ js }}"></script>
{% endfor %}
{% if page.getCustom('canonical') %}
<link rel="canonical" href="{{ page.getUrl(page.getCustom('canonical')) }}" />
{% endif %}
{% if page.getCustom('robots') %}
<meta name="robots" content="{{ page.getCustom('robots') }}" />
{% endif %}
{% if page.getCustom('inline_css') %}
<style>
{{ page.getCustom('inline_css') }}
</style>
{% endif %}